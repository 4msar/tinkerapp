{
    "name": "4msar/tinkerapp",
    "description": "Simple tinker app for PHP developers.",
    "keywords": ["framework", "laravel", "laravel zero", "console", "cli", "pjp"],
    "homepage": "https://github.com/4msar/tinkerapp",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/4msar/tinkerapp/issues",
        "source": "https://github.com/4msar/tinkerapp"
    },
    "authors": [
        {
            "name": "Saiful Alam",
            "email": "hello@msar.me"
        }
    ],
    "require": {
        "php": "^8.2",
        "intonate/tinker-zero": "^1.2",
        "laravel-zero/framework": "^12.0.2"
    },
    "require-dev": {
        "laravel/pint": "^1.25.1",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.8.4|^4.1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["cli"]
}
